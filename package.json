{
  "name": "heart-rate-monitor-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "android": "EXPO_PUBLIC_MDS_LIB_PATH=../android/libs npx expo run:android --device",
    "prebuild": "rm -rf android && rm -rf ios && EXPO_PUBLIC_MDS_LIB_PATH=../android/libs npx expo prebuild",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "expo": "~50.0.14",
    "expo-mds": "0.5.0",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-ble-plx": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
